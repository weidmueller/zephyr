/dts-v1/;

#include <arm/xilinx/zynqmp_rpu.dtsi>

/ {
	model = "Xilinx ZCU102";
	compatible = "xlnx,zynqmp";

	chosen {
		zephyr,flash      = &flash0;
		zephyr,sram       = &sram0;
		zephyr,console    = &uart0;
		zephyr,shell-uart = &uart0;
	};
};

&gem3 {
	status = "okay";
	local-mac-address = [00 80 41 00 00 01];
};

&ttc0 {
	status = "okay";
	clock-frequency = <100000000>;
};

&uart0 {
	status = "okay";
	current-speed = <115200>;
	clock-frequency = <100000000>;
};

